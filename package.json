{
	"name": "phil.red",
	"version": "1.0.0",
	"description": "flying-sheepâ€™s blog",
	"private": true,
	"main": "dist/bundle.js",
	"type": "module",
	"repository": "https://github.com/flying-sheep/phil.red.git",
	"author": "Philipp A. <flying-sheep@web.de>",
	"license": "GPL-3.0",
	"scripts": {
		"start": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"bundlesize": "tsc && vite-bundle-visualizer",
		"lint": "$npm_execpath run lint:check && $npm_execpath run lint:deps",
		"lint:ci": "biome ci && $npm_execpath run lint:deps",
		"lint:check": "biome check --write .",
		"lint:deps": "depcheck --ignores=unfonts.css,@fontsource/iosevka,@fontsource/iosevka-aile,@tsconfig/strictest,@total-typescript/ts-reset,@arborium/ini,@arborium/python,@arborium/toml,@arborium/typescript,@arborium/xml,@arborium/zsh",
		"deploy": "$npm_execpath run build && rsync --recursive --delete --archive --compress --progress dist/ root@phil.red:/srv/http/phil.red",
		"prepare": "husky"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@arborium/arborium": "^2.12.4",
		"@arborium/ini": "^2.12.4",
		"@arborium/python": "^2.12.4",
		"@arborium/toml": "^2.12.4",
		"@arborium/typescript": "^2.12.4",
		"@arborium/xml": "^2.12.4",
		"@arborium/zsh": "^2.12.4",
		"@dr.pogodin/react-helmet": "^3.0.5",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@fontsource/iosevka": "^5.2.5",
		"@fontsource/iosevka-aile": "^5.2.5",
		"@mui/icons-material": "^7.3.7",
		"@mui/material": "^7.3.7",
		"@mui/system": "^7.0.1",
		"merge-sx": "^3.4.0",
		"notistack": "^3.0.2",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"react-error-boundary": "^6.1.0",
		"react-is": "^19.2.4",
		"react-portal-target": "^1.0.2",
		"react-router": "^7.13.0",
		"react-slide-routes": "^3.1.0",
		"react-vega": "^8.0.0",
		"temml": "^0.13.1",
		"typed-jsx": "^0.1.1",
		"vega": "^6.2.0",
		"vega-embed": "^7.1.0",
		"vega-lite": "^6.4.2",
		"vega-util": "^2.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@total-typescript/ts-reset": "^0.6.1",
		"@tsconfig/strictest": "^2.0.8",
		"@types/estree": "^1.0.8",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^25.0.9",
		"@types/react": "^19.2.10",
		"@types/react-dom": "^19.2.3",
		"@types/react-is": "^19.2.0",
		"@vitejs/plugin-react": "^5.1.2",
		"@yarnpkg/fslib": "^3.1.4",
		"@yarnpkg/libzip": "^3.2.2",
		"depcheck": "^1.4.7",
		"estree-walker": "^3.0.3",
		"globby": "^16.1.0",
		"husky": ">=9.1.7",
		"lint-staged": ">=16.2.7",
		"magic-string": "^0.30.21",
		"markdown-it": "^14.1.0",
		"pyodide": "^0.29.3",
		"react-base16-styling": "^0.10.0",
		"react-json-tree": "^0.20.0",
		"rollup": "^4.55.1",
		"typescript": "^5.9.3",
		"unplugin-fonts": "^1.4.0",
		"vite": "^7.3.1",
		"vite-bundle-visualizer": "^1.2.1"
	},
	"packageManager": "yarn@4.12.0",
	"//": "https://github.com/yarnpkg/berry/issues/3366",
	"resolutions": {
		"node-gyp": "npm:optional-pkg@1"
	},
	"lint-staged": {
		"!(rawPosts)": "biome check --no-errors-on-unmatched --files-ignore-unknown=true --write"
	}
}
